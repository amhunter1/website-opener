{
  "name": "website-opener",
  "version": "1.0.0",
  "description": "Open multiple websites in normal or incognito/private windows via a beautiful UI.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --watch server.js --watch public --ext js,html,css --exec \"node server.js\""
  },
  "keywords": [
    "website",
    "opener",
    "incognito",
    "private",
    "node",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.19.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}


